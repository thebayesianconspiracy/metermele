{
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "comma-dangle": ["error", {
      "arrays": "never",
      "objects": "never",
      "imports": "never",
      "exports": "never",
      "functions": "never"
    }],
    "no-shadow": 0,
    "camelcase": 0,
    "no-underscore-dangle": 0,
    "no-param-reassign": 0
  }
}
